<UserControl x:Class="Silverlight.ProcessEditor.Elements.CloseElement"
              xmlns:my="clr-namespace:Silverlight.ProcessEditor.Elements"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="300" d:DesignWidth="400" Width="64" Height="64">
    
    <Grid x:Name="LayoutRoot">
        <Image HorizontalAlignment="Stretch" MouseEnter="imgClose_MouseEnter" MouseLeave="imgClose_MouseLeave" MouseLeftButtonUp="imgClose_MouseLeftButtonUp"
               Margin="0,0,0,0" Name="imgClose" Stretch="Fill" 
               VerticalAlignment="Stretch" Source="/JM.Workflow.Sl;component/Source/close.png" />
    </Grid>
</UserControl>
